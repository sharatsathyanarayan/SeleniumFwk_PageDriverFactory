<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="FrameWorkTwo" parallel="tests">
    <test name="LoginTestChrome" parallel="classes">
    <parameter name="browsername" value="chrome"/>
        <classes>
            <class name="frameworktwo.base.test.TestLogin"/>
            <class name="frameworktwo.base.test.TestLogin_negative"/>
            <class name="frameworktwo.base.test.ProductsTest"/>
        </classes>
    </test>
    <test name="LoginTestFireFox" parallel="classes">
    <parameter name="browsername" value="firefox"/>
        <classes>
            <class name="frameworktwo.base.test.TestLogin"/>
            <class name="frameworktwo.base.test.TestLogin_negative"/>
            <class name="frameworktwo.base.test.ProductsTest"/>
        </classes>
    </test>
</suite>